  <Sequence sap2010:Annotation.AnnotationText="Parses a XAML file into a tree representation, returning the tree and the associated namespace manager. The namespace manager is required to make correct XPath queries in the checks." DisplayName="ParseXAML" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <Assign DisplayName="Assign out_XMLTree" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="sx:XmlDocument">[out_XMLTree]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sx:XmlDocument">[New XmlDocument()]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign out_NamespaceManager" sap2010:WorkflowViewState.IdRef="Assign_2">
      <Assign.To>
        <OutArgument x:TypeArguments="sx:XmlNamespaceManager">[out_NamespaceManager]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sx:XmlNamespaceManager">[New XmlNamespaceManager(out_XMLTree.NameTable)]</InArgument>
      </Assign.Value>
    </Assign>
    <InvokeMethod DisplayName="Create XML tree" sap2010:WorkflowViewState.IdRef="InvokeMethod_1" MethodName="LoadXml">
      <InvokeMethod.TargetObject>
        <InArgument x:TypeArguments="sx:XmlDocument">[out_XMLTree]</InArgument>
      </InvokeMethod.TargetObject>
      <InArgument x:TypeArguments="x:String">[in_XAMLContents]</InArgument>
    </InvokeMethod>
    <InvokeMethod sap2010:Annotation.AnnotationText="The default namespace needs to be specified for later usage of XPath queries: https://docs.microsoft.com/en-us/dotnet/standard/data/xml/xpath-queries-and-namespaces#the-default-namespace&#xA;&#xA;In this case, the default namespace can be referenced by the prefix &quot;xaml&quot;." DisplayName="Add the default namespace" sap2010:WorkflowViewState.IdRef="InvokeMethod_3" MethodName="AddNamespace">
      <InvokeMethod.TargetObject>
        <InArgument x:TypeArguments="sx:XmlNamespaceManager">[out_NamespaceManager]</InArgument>
      </InvokeMethod.TargetObject>
      <InArgument x:TypeArguments="x:String">xaml</InArgument>
      <InArgument x:TypeArguments="x:String">http://schemas.microsoft.com/netfx/2009/xaml/activities</InArgument>
    </InvokeMethod>
    <Sequence sap2010:Annotation.AnnotationText="These namespaces are explicitly added to prevent errors when a particular check searches for elements in a namespace, but one of the workflows checked does not declare that namespace." DisplayName="Add commonly used namespaces" sap2010:WorkflowViewState.IdRef="Sequence_3">
      <InvokeMethod sap2010:Annotation.AnnotationText="Add the commonly used UI namespace: &#xA;xmlns:ui=&quot;http://schemas.uipath.com/workflow/activities&quot;" DisplayName="Add UI namespace" sap2010:WorkflowViewState.IdRef="InvokeMethod_4" MethodName="AddNamespace">
        <InvokeMethod.TargetObject>
          <InArgument x:TypeArguments="sx:XmlNamespaceManager">[out_NamespaceManager]</InArgument>
        </InvokeMethod.TargetObject>
        <InArgument x:TypeArguments="x:String">ui</InArgument>
        <InArgument x:TypeArguments="x:String">http://schemas.uipath.com/workflow/activities</InArgument>
      </InvokeMethod>
      <InvokeMethod sap2010:Annotation.AnnotationText="Add the commonly used SAP namespace: &#xA;xmlns:sap2010=&quot;http://schemas.microsoft.com/netfx/2010/xaml/activities/presentation&quot;" DisplayName="Add SAP2010 namespace" sap2010:WorkflowViewState.IdRef="InvokeMethod_5" MethodName="AddNamespace">
        <InvokeMethod.TargetObject>
          <InArgument x:TypeArguments="sx:XmlNamespaceManager">[out_NamespaceManager]</InArgument>
        </InvokeMethod.TargetObject>
        <InArgument x:TypeArguments="x:String">sap2010</InArgument>
        <InArgument x:TypeArguments="x:String">http://schemas.microsoft.com/netfx/2010/xaml/activities/presentation</InArgument>
      </InvokeMethod>
    </Sequence>
    <ui:ForEach x:TypeArguments="sx:XmlAttribute" sap2010:Annotation.AnnotationText="Add other namespaces defined in the workflow file." DisplayName="For each attribute in root element" sap2010:WorkflowViewState.IdRef="ForEach`1_1" Values="[out_XMLTree.DocumentElement.Attributes]">
      <ui:ForEach.Body>
        <ActivityAction x:TypeArguments="sx:XmlAttribute">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="sx:XmlAttribute" Name="Attribute" />
          </ActivityAction.Argument>
          <If Condition="[Attribute.Name.StartsWith(&quot;xmlns:&quot;)]" DisplayName="If attribute defines a namespace" sap2010:WorkflowViewState.IdRef="If_1">
            <If.Then>
              <InvokeMethod DisplayName="Add namespaces to manager" sap2010:WorkflowViewState.IdRef="InvokeMethod_2" MethodName="AddNamespace">
                <InvokeMethod.TargetObject>
                  <InArgument x:TypeArguments="sx:XmlNamespaceManager">[out_NamespaceManager]</InArgument>
                </InvokeMethod.TargetObject>
                <InArgument x:TypeArguments="x:String">[Attribute.Name.Substring(Attribute.Name.IndexOf(":")+1)]</InArgument>
                <InArgument x:TypeArguments="x:String">[Attribute.Value]</InArgument>
              </InvokeMethod>
            </If.Then>
          </If>
        </ActivityAction>
      </ui:ForEach.Body>
    </ui:ForEach>
    <sads:DebugSymbol.Symbol></sads:DebugSymbol.Symbol>
